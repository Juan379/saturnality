<% if !current_user %>
    <h1>You're not currently signed in</h1>
    <%= link_to 'Sign up', new_user_registration_path %> <br/>
    <%= link_to 'Log in', new_user_session_path %>
<% else %>
    <h1>Hello, <%= current_user.name %> </h1> 
    <%= link_to "Log Out", destroy_user_session_path, :method => :delete %> <br/><br/><br/>
    <%= link_to 'Parties', parties_url %> <br/>
    <%= link_to 'Services', services_url %> 
<% end %>



